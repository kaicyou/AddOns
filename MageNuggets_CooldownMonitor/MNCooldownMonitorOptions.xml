<Ui xmlns="http://www.blizzard.com/wow/ui/" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:xsd="http://www.w3.org/2001/XMLSchema">
 <Script file="MNCooldownMonitorOptions.lua" ></Script>

<Frame name="MageNuggetsOptionsOnLoadFrame">
    <Scripts>
        <OnLoad>
            self.TimeSinceLastUpdate = 0;
            MageNuggetCooldownMonitorOptions_Start(self);
        </OnLoad>
        <OnUpdate>
        </OnUpdate>
        <OnEvent>
            MageNuggetsCooldownMonitorOptions_OnEvent(self, event, ...);
        </OnEvent>
    </Scripts>
</Frame>

<Frame name="MageNugCDOptionsFrame" toplevel="true" hidden="false" parent="MageNugCDOptions">
    <Size>
        <AbsDimension x="480" y="325"/>
    </Size>
    <Anchors>
        <Anchor point="TOPLEFT"/>
        <Anchor point="BOTTOMRIGHT"/>
    </Anchors>
    <Layers>
        <Layer level="ARTWORK">
            <FontString name="$parent_IgnoreLabelText" inherits="GameFontNormal" text="Monitor Size">
                <Anchors>
                    <Anchor point="TOPLEFT" relativeTo="$parent">
                        <Offset>
                            <AbsDimension x="75" y="-32"/>
                        </Offset>
                    </Anchor>
                </Anchors>
            </FontString>
            <FontString name="$parent_LockFrameText" inherits="GameFontNormal" text="Lock Frame">
                <Anchors>
                    <Anchor point="TOPLEFT" relativeTo="$parent">
                        <Offset>
                            <AbsDimension x="50" y="-89"/>
                        </Offset>
                    </Anchor>
                </Anchors>
            </FontString>
            <FontString name="$parent_IgnoreLabelText" inherits="GameFontNormal" text="IGNORE LIST:">
                <Anchors>
                    <Anchor point="TOPLEFT" relativeTo="$parent">
                        <Offset>
                            <AbsDimension x="18" y="-155"/>
                        </Offset>
                    </Anchor>
                </Anchors>
            </FontString>
            <FontString name="$parent_NoteText" font="Fonts\FRIZQT__.TTF" text="Spell names are case sensitive.">
                <Anchors>
                    <Anchor point="TOPLEFT" relativeTo="$parent">
                        <Offset>
                            <AbsDimension x="18" y="-196"/>
                        </Offset>
                    </Anchor>
                </Anchors>
                <FontHeight>
                  <AbsValue val="12"/>
                </FontHeight>
                <Color r="1" g="0.8" b="1"/>
            </FontString>
            <FontString name="$parent_IgnoreListText" inherits="GameFontNormal" text="None">
                <Anchors>
                    <Anchor point="TOPLEFT" relativeTo="$parent">
                        <Offset>
                            <AbsDimension x="15" y="-240"/>
                        </Offset>
                    </Anchor>
                </Anchors>
                <Scripts>
                  <OnLoad>
                  </OnLoad>
              </Scripts>
            </FontString>
        </Layer>
    </Layers>
    <Frames>
      <Slider name="$parent_SizeSlider" inherits="OptionsSliderTemplate">
        <Size>
            <AbsDimension x="200" y="20"/>
        </Size>
        <Anchors>
            <Anchor point="TOPLEFT" relativeTo="$parent">
              <Offset>
                <AbsDimension x="30" y="-45"/>
              </Offset>
            </Anchor>
        </Anchors>
        <Scripts>
          <OnLoad>
              getglobal(self:GetName().."High"):SetText("9");
              getglobal(self:GetName().."Low"):SetText("0");
              self:SetMinMaxValues(0,9);
              self:SetValueStep(1);
          </OnLoad>
          <OnValueChanged>
              MNcooldownSlider();
          </OnValueChanged>
        </Scripts>
      </Slider>
      <CheckButton name="$parent_CheckButtonClickThroughFrame" inherits="OptionsCheckButtonTemplate">
-           <Anchors>
-               <Anchor point="TOPLEFT" relativeTo="$parent">
-                   <Offset>
                      <AbsDimension x="20" y="-85" />
                  </Offset>
              </Anchor>
          </Anchors>
-           <Scripts>
              <OnLoad>
              </OnLoad>
              <OnClick>
                  MnCdMonClickThrough();
              </OnClick>
          </Scripts>
      </CheckButton>
      <EditBox name="$parent_AddSpellEditBox" inherits="InputBoxTemplate">
          <Size>
              <AbsDimension x="200" y="26"/>
          </Size>
          <Anchors>
              <Anchor point="TOPLEFT">
                  <Offset>
                      <AbsDimension x="20" y="-172"/>
                  </Offset>
              </Anchor>
          </Anchors>
          <Scripts>
              <OnShow>
                  MageNugCDOptionsFrame_AddSpellEditBox:SetCursorPosition(0);
                  MageNugCDOptionsFrame_AddSpellEditBox:SetAutoFocus(true);
              </OnShow>
              <OnCursorChanged>
              </OnCursorChanged>
              <OnEnter>
              </OnEnter>
              <OnLeave>
              </OnLeave>
          </Scripts>
        </EditBox>
        <Button name="$parent_AddButton" inherits="OptionsButtonTemplate" text="+">
         	<Size>
          		<AbsDimension x="23" y="23"></AbsDimension>
          </Size>
          <Anchors>
            <Anchor point="TOPLEFT" relativeTo="$parent">
           		<Offset>
		            <AbsDimension x="230" y="-173"/>
	            </Offset>
            </Anchor>
          </Anchors>
          <Scripts>
            <OnClick>
              MNaddSpellToIgnoreListFromEditBox();
            </OnClick>
          </Scripts>
        </Button>
        <Button name="$parent_RemoveButton" inherits="OptionsButtonTemplate" text="-">
         	<Size>
          		<AbsDimension x="23" y="23"></AbsDimension>
          </Size>
          <Anchors>
            <Anchor point="TOPLEFT" relativeTo="$parent">
           		<Offset>
		            <AbsDimension x="255" y="-173"/>
	            </Offset>
            </Anchor>
          </Anchors>
          <Scripts>
            <OnClick>
              MNRemoveSpellFromEditBox();
            </OnClick>
          </Scripts>
        </Button>
    </Frames>
    <Scripts>
    </Scripts>
</Frame>


</Ui>
