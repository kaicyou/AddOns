<Ui xmlns="http://www.blizzard.com/wow/ui/" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:xsd="http://www.w3.org/2001/XMLSchema">
 <Script file="MageNuggetsCooldownMonitor.lua" ></Script>

 <Frame name="MageNuggetsCooldownMonitor">
    <Scripts>
		    <OnLoad>
            self.TimeSinceLastUpdate = 0
            mageNuggetsCdMon_OnStart(self);
        </OnLoad>
		    <OnUpdate>
            MageNuggets_Cooldown_OnUpdate(self, elapsed)
        </OnUpdate>
		    <OnEvent>
            MageNuggetsCdMon_OnEvent(self, event, ...);
        </OnEvent>
	  </Scripts>
 </Frame>


<Frame name="MageNugCD_Frame" parent="UIParent" hidden="false" enableMouse="true" movable="true">
    <Size x="100" y="20" />
    <Anchors>
        <Anchor point="CENTER" />
    </Anchors>
    <Layers>
        <Layer level="ARTWORK">
        </Layer>
    </Layers>
    <Frames>
    </Frames>
    <Scripts>
      <OnLoad>
        self:RegisterForDrag("LeftButton");
        self.TimeSinceLastUpdate = 0
      </OnLoad>
	    <OnDragStart>
        --MNLockFrames(self);
        self:StartMoving();
        self.isMoving = true;
      </OnDragStart>
	    <OnDragStop>
        self:StopMovingOrSizing();
        self.isMoving = false;
        --  MageNuggets_Frames.cd_point, _, MageNuggets_Frames.cd_relativePoint, MageNuggets_Frames.cd_xOfs, MageNuggets_Frames.cd_yOfs = MageNugCD_Frame:GetPoint();
      </OnDragStop>
      <OnEnter>
			  --MNcooldownOnEnter();
      </OnEnter>
      <OnLeave>
        --GameTooltip:Hide()
      </OnLeave>
    </Scripts>
</Frame>

<Frame name="MageNugCD1_Frame" parent="MageNugCD_Frame" hidden="true">
    <Size x="100" y="12" />
    <Anchors>
        <Anchor point="CENTER" relativeTo="$parent">
        <Offset>
            <AbsDimension x="0" y="-4"/>
        </Offset>
        </Anchor>
    </Anchors>
    <Layers>
        <Layer level="BACKGROUND">
            <Texture name="$parent_Texture" file="Interface\Icons\Spell_frost_coldhearted">
                <Size>
                    <AbsDimension x="12" y="12"/>
                </Size>
                <Anchors>
                    <Anchor point="LEFT"/>
                </Anchors>
                    <TexCoords left="0" right="1" top="0" bottom="1"/>
            </Texture>
        </Layer>
        <Layer level="ARTWORK">
            <FontString name="$parent_Text" font="Fonts\FRIZQT__.TTF" inherits="GameFontNormal">
                <Anchors>
                    <Anchor point="LEFT">
                        <Offset>
                           <AbsDimension x="13" y="0"/>
                        </Offset>
                    </Anchor>
                </Anchors>
                <FontHeight>
                   <AbsValue val="8"/>
                </FontHeight>
                <color r="1" g="1" b="1"/>
            </FontString>
            <FontString name="$parent_Text2" font="Fonts\FRIZQT__.TTF" inherits="GameFontNormal">
                <Anchors>
                    <Anchor point="RIGHT">
                        <Offset>
                           <AbsDimension x="5" y="0"/>
                        </Offset>
                    </Anchor>
                </Anchors>
                <FontHeight>
                   <AbsValue val="8"/>
                </FontHeight>
                <color r="1" g="1" b="1"/>
            </FontString>
        </Layer>
    </Layers>
    <Frames>
        <StatusBar name="$parent_Bar" frameStrata="LOW" inherits="TextStatusBar">
            <Size x="99" y="12"/>
            <Anchors>
                <Anchor point="center" relativeTo="$parent">
                    <Offset>
                        <AbsDimension x="12" y="0"/>
                    </Offset>
                </Anchor>
            </Anchors>
            <BarTexture file="Interface\TargetingFrame\UI-StatusBar"/>
            <Scripts>
                <OnLoad>
                   self:SetStatusBarColor(1, 0, 0)
                   self:SetMinMaxValues(0,13)
                </OnLoad>
            </Scripts>
        </StatusBar>
    </Frames>
    <Scripts>
        <OnLoad>
            self:SetScript("OnMouseUp", function(self, button, down)
                if(button == "RightButton") then
                    addSpellToIgnoreList(MageNugCD1_Frame_Text:GetText());
                end
            end)
            self:RegisterForDrag("LeftButton");
            self.TimeSinceLastUpdate = 0;
            self:EnableMouse(true);
        </OnLoad>
        <OnUpdate>
           MageNuggetsCD1_OnUpdate(self, elapsed)
        </OnUpdate>
    </Scripts>
</Frame>

